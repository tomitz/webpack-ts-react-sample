{
    "extends": [
        "tslint-config-airbnb",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "interface-name": false,
        "no-unused-expression": [true, "allow-fast-null-checks"],
        "variable-name": [true, "allow-leading-underscore", "allow-pascal-case"]
    }
}
